@model string

<script src="~/js/Angular/Controllers/@(Model).js"></script>

<script type="text/javascript">
    "use strict";

     const controllerConstructor = @(Model + "Controller");

    @if(ViewBag.data != null)
    {
        @:let data = @Html.Raw(ViewBag.data ?? "{}");
        @:controllerConstructor(data);
    }
    else
    {
        @:controllerConstructor();
    }
</script>

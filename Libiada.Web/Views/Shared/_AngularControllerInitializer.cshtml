@model string

<script src="~/js/Angular/Controllers/@(Model).js" asp-append-version="true"></script>


<script type="text/javascript">
    "use strict";

     var controllerConstructor = @(Model + "Controller");

    @if(ViewBag.data != null)
    {
        @:var data = @Html.Raw(ViewBag.data);
        @:controllerConstructor(data);
    }
    else
    {
        @:controllerConstructor();
    }
</script>
